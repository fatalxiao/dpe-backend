{
    "presets": [
        [
            "env",
            {
                "targets": {
                    "node": "current"
                }
            }
        ]
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "root": [
                    "./src"
                ],
                "extensions": [
                    ".js"
                ]
            }
        ],
        "transform-export-extensions",
        "transform-decorators-legacy"
    ]
}
