        "babel-plugin-transform-decorators-legacy": "^1.3.5",
        "koa": "^2.5.3",
        "koa-bodyparser": "^4.2.1",
        "koa-router": "^7.4.0",
        "koa-static": "^5.0.0",
        "lodash": "^4.17.11",
        "moment": "^2.22.2",
        "mysql2": "^1.6.1",
        "node-xlsx": "^0.12.1",
        "sequelize": "^4.39.0"
    },
