{
    "name": "dpe-backend",
    "author": "liangxiaojun",
    "version": "1.0.0",
    "description": "DPE Back-end Project.",
    "main": "./src/app.js",
    "scripts": {
        "dev": "nodemon src/app --exec babel-node $NODE_DEBUG_OPTION",
        "clean": "rimraf dist",
        "package": "cross-env NODE_ENV=production gulp package",
        "build": "npm run clean && npm run package",
        "run": "cross-env NODE_ENV=production node dist/app",
        "start": "npm run build && npm run run"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/fatalxiao/dpe-backend"
    },
    "keywords": [
        "dural-puncture-epidural",
        "dpe",
        "koa2",
        "sequelize",
        "mysql"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/fatalxiao/dpe-backend/issues"
    },
    "dependencies": {
        "@koa/cors": "^2.2.2",
        "babel-plugin-transform-decorators-legacy": "^1.3.5",
        "koa": "^2.5.3",
        "koa-bodyparser": "^4.2.1",
        "koa-router": "^7.4.0",
        "koa-static": "^5.0.0",
        "lodash": "^4.17.11",
        "moment": "^2.22.2",
        "mysql2": "^1.6.1",
        "node-xlsx": "^0.12.1",
        "sequelize": "^4.39.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-plugin-module-resolver": "^3.1.1",
        "babel-plugin-transform-export-extensions": "^6.22.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "cross-env": "^5.2.0",
        "gulp": "^3.9.1",
        "gulp-babel": "^8.0.0",
        "gulp-sequence": "^1.0.0",
        "mocha": "^5.2.0",
        "nodemon": "^1.18.4",
        "rimraf": "^2.6.2"
    },
    "nodemonConfig": {
        "ignore": [
            "src/swagger/*"
        ]
    }
}
